<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <sites>
            <site name="newsBlog" patch:before="site[@name='website']"
                  hostName ="newsBlog"
                  targetHostName="newsBlog" 
                  scheme="http" 
                  virtualFolder="/"
                  physicalFolder="/"
                  rootPath="/sitecore/content/blog"
                  startItem="/home"
                  database="web"
                  domain="extranet"
                  allowDebug="true"
                  cacheHtml="false"
                  htmlCacheSize="50MB"
                  enablePreview="true"
                  enableWebEdit="true"
                  enableDebugger="true"
                  disableClientData="false"
                  enableItemLanguageFallback="true"
                  enableFieldLanguageFallback="true"
                  enforceVersionPresence="true"
                  dictionaryDomain="Global Dictionary" />

        </sites>
    </sitecore>
</configuration>
