@model IEnumerable<InfrastructureModule.Models.Pages.Post.PostItemModel>


<div style="overflow: hidden">
    @foreach (var post in Model)
    {
        <div class="col-md-4">
            <h2>@post.Title</h2>
            <p>@post.Subtitle</p>
            <p><a class="btn btn-primary" href="@post.Url" role="button">@Sitecore.Globalization.Translate.Text("L.Link")</a>
            </p>
        </div>
    }
</div>
