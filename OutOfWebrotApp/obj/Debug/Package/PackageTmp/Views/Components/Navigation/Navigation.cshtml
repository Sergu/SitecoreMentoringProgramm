@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model OutOfWebrotApp.Models.Components.Navigation.NavigationModel



<ul class="nav navbar-nav">
    @foreach (var navigationItem in Model.NavigationItems)
    {
        <li class=""><a href="@navigationItem.Url">@navigationItem.Title</a></li>
    }
    
    @Html.Action("Language","Navigation")
</ul>
