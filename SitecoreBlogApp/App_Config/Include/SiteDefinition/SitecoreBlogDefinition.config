<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <sites>
            
            <site name="sitecoreBlog" patch:before="site[@name='website']"
                  hostName ="sitecoreBlog"
                  virtualFolder="/"
                  physicalFolder="/"
                  rootPath="/sitecore/content/sitecoreblog"
                  startItem="/home"
                  database="web"
                  domain="extranet"
                  allowDebug="true"
                  cacheHtml="false"
                  htmlCacheSize="50MB"
                  enablePreview="true"
                  enableWebEdit="true"
                  enableDebugger="true"
                  disableClientData="false"
                  enableItemLanguageFallback="true"
                  enableFieldLanguageFallback="true"
                  enforceVersionPresence="true"
                  dictionaryDomain="SitecoreBlogDictionary" />
        </sites>
    </sitecore>
</configuration>
